import java.util.*;
class sumoftwo
{
public static void main(String args[])
{
	Scanner scan=new Scanner(System.in);
	System.out.print("Enter the number:") ;
	int a=scan.nextInt();
	System.out.print("Enter the number:");
	int b=scan.nextInt();
  System.out.println("Sum of two numbers : "+(a+b));
}
}
